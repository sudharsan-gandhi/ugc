<div class="col-md-4">
<form class="form-group" action="/user/auth" method="post" id="form">
	<label for="email">Email</label>
		<input class="form-control" type="text" required="required" name="email" maxlength="50" id="email">
	<label for="password">Password</label>
		<input class="form-control" type="password" required="required" name="password" maxlength="25" id="password" >
		<br>
	<div class="button-group" role="group">
		<button type="button" class="col-md-5 btn btn-lg btn-primary">Login</button>
		<div class="col-md-2"></div>
		<button type="reset" class="col-md-5 btn btn-lg btn-danger">Reset</button>
	</div>
</form>

<a href="/user/new" class="col-md-12 btn btn-lg btn-default" style="margin-top: 5px">new user?</a>
